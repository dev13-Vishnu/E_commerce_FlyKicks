    <main class="main">
      
      
      <section class="product-tabs section-padding wow fadeIn animated">
        <div class="container">
          
          <!--End nav-tabs-->
          <div class="tab-content wow fadeIn animated" id="myTabContent">
            <div
              class="tab-pane fade show active"
              id="tab-one"
              role="tabpanel"
              aria-labelledby="tab-one"
            >
              <div class="row product-grid-4">
                <% products.forEach(product => {%>
                    <div class="col-lg-4 col-md-6  col-12 col-sm-6">
                        <div class="product-cart-wrap mb-30">
                          <div class="product-img-action-wrap">
                            <div class="product-img product-img-zoom">
                              <a href="/product/detail?id=<%= product._id %>">
                                <img
                                  class="default-img"
                                  src="/<%= product.image[0].replace(/\\/g, '/') %>"
                                  alt=""
                                />
                                <img
                                  class="hover-img"
                                  src="/<%= product.image[1].replace(/\\/g, '/') %>"
                                  alt=""
                                />
                              </a>
                            </div>
                            <div class="product-action-1">
                              <a
                                aria-label="Quick view"
                                class="action-btn hover-up"
                                data-bs-toggle="modal"
                                data-bs-target="#quickViewModal"
                                ><i class="fi-rs-eye"></i
                              ></a>
                              <a
                                aria-label="Add To Wishlist"
                                class="action-btn hover-up add-to-wishlist" 
                                data-product-id = "<%=product._id %>"
                                ><i class="fi-rs-heart"></i
                              ></a>
                              <a
                                aria-label="Compare"
                                class="action-btn hover-up"
                                href="shop-compare.html"
                                ><i class="fi-rs-shuffle"></i
                              ></a>
                            </div>
                            <div
                              class="product-badges product-badges-position product-badges-mrg"
                            >
                              <span class="hot">Hot</span>
                            </div>
                          </div>
                          <div class="product-content-wrap">
                            <div class="product-category">
                              <a href="shop-grid-right.html">Clothing</a>
                            </div>
                            <h2>
                              <a class="item-name" href="/product/detail?id=<%= product._id %>"
                                ><%=product.name%></a
                              >
                            </h2>
                            <div class="rating-result" title="100%">
                              <span>
                                <span>90%</span>
                              </span>
                            </div>
                            <div class="product-price">
                              <% if (product.discountedPrice < product.price) { %>
                                <!-- Display discounted price with two decimal places -->
                                <span id="discountedPrice-<%= product._id %>">₹<%= product.discountedPrice.toFixed(2) %>/-</span>
                                
                                <!-- Display old price with two decimal places -->
                                <span class="old-price">₹<%= product.price.toFixed(2) %>/-</span>
                                
                                <!-- Show offer name and discount percentage -->
                                <div class="offer-details" style="font-size: .9rem;">
                                  <p>Offer: <%= product.appliedOffer %> (<%= 
                                    Math.round(((product.price - product.discountedPrice) / product.price) * 100) 
                                  %>%)</p>
                                </div>
                            
                              <% } else { %>
                                <!-- Display original price if no discount is applied -->
                                <span id="price-<%= product._id %>">₹<%= product.price.toFixed(2) %>/-</span>
                              <% } %>
                            </div>
                            
                            <div class="product-action-1 show">
                              <a
